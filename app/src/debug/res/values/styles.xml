<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="colorTranslucent">#d2ffffff</item>
        <item name="colorGray">#9e9e9e</item>
        <item name="android:windowBackground">@color/main_background</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@android:color/black</item>
        <item name="colorLine">#CCCCCC</item>
        <item name="colorCircle">#AAAAAA</item>
        <item name="colorSave">#4B4B4B</item>
        <item name="colorCard">#FFFFFF</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="alphaBackground">#CCFFFFFF</item>
        <item name="autoCompleteTextViewStyle">@style/Cursor</item>
        <item name="selectable_library_drawable">@drawable/library_item_selector_light</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="AppThemeDark" parent="Theme.AppCompat.NoActionBar">
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="colorTranslucent">#d2414141</item>
        <item name="colorGray">#bdbdbd</item>
        <item name="colorLine">#666666</item>
        <item name="colorCircle">#666666</item>
        <item name="colorSave">#C0C0C0</item>
        <item name="colorCard">#ff303030</item>
        <item name="alphaBackground">#CC000000</item>
        <item name="autoCompleteTextViewStyle">@style/Cursor</item>
        <item name="selectable_library_drawable">@drawable/library_item_selector_dark</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat</item>
    </style>

    <style name="AppThemeBlack" parent="Theme.AppCompat.NoActionBar">
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="colorPrimary">@android:color/black</item>
        <item name="colorTranslucent">#d2000000</item>
        <item name="colorGray">#757575</item>
        <item name="colorLine">#666666</item>
        <item name="colorCircle">#666666</item>
        <item name="colorSave">#C0C0C0</item>
        <item name="colorCard">#000000</item>
        <item name="alphaBackground">#CC000000</item>
        <item name="autoCompleteTextViewStyle">@style/Cursor</item>
        <item name="selectable_library_drawable">@drawable/library_item_selector_amoled</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat</item>
    </style>

